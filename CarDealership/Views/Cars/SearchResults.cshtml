@using Dealership.Models;

<h1>CarDealership</h1>
<ul>

@if (Model.Count == 0)
{
  <p>You have no cars in your list</p>
}
@if(Model.Count != 0)
{
@foreach (Car car in Model)
{
    <li>@car.GetMakeModel() @car.GetPrice() @car.GetMiles() @car.GetMessage()</li>
}
}
</ul>
<a href="/cars">Go back to home</a>
